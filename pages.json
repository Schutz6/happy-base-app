{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^module-(.*)": "@/modules/module-$1/module-$1.vue"
		}
	},
	"pages": [
		{
		    "path" : "pages/common/login/login",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "登录",
		        "enablePullDownRefresh": false,
				"navigationStyle": "custom"
		    }
		},
		{
		    "path" : "pages/common/register/register",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "注册",
		        "enablePullDownRefresh": false,
				"navigationStyle": "custom"
		    }
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		},
        {
        	"path": "pages/me/me",
        	"style": {
        		"navigationBarTitleText": "我的",
        		"enablePullDownRefresh": false,
				"navigationStyle": "custom"
        	}
        }
        
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
		"color": "#266CAC",
		"selectedColor": "#00E0FF",
		"backgroundColor": "#161C33",
		"borderStyle": "#294D6B",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/tabbar/icon-index.png",
				"selectedIconPath": "static/tabbar/icon-index-select.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/me/me",
				"iconPath": "static/tabbar/icon-me.png",
				"selectedIconPath": "static/tabbar/icon-me-select.png",
				"text": "我的"
			}
		]
	}
}
